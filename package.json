{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "connect-history-api-fallback": "^1.5.0",
    "copyfiles": "^2.1.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.3",
    "rollup": "^0.66.2",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sass": "^1.1.0",
    "rollup-plugin-svelte": "^4.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "svelte": "^2.6.0"
  },
  "scripts": {
    "clean": "rimraf public/",
    "copy:index": "copyfiles -u 1 src/index.html public",
    "init": "run-p clean && run-p copy:index",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p init && run-p autobuild"
  }
}
